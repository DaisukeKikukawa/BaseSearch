<%= search_form_for @q, url: admin_team_categories_path, html: { method: :get } do |f| %>
  <div class="d-flex flex-wrap">
    <div class="form-group mr-3">
      <%= f.label :id_eq, 'ID' %>
      <%= f.search_field :id_eq %>
    </div>
    <div class="form-group mr-3">
      <%= f.label :name_cont, 'チームカテゴリー名' %>
      <%= f.search_field :name_cont %>
    </div>
  </div>

  <%= f.submit '検索', class: 'btn btn-primary' %>
  <%= link_to 'リセット', admin_team_categories_path, class: 'btn btn-secondary' %>
<% end %>